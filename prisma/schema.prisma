// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id Int @default(autoincrement()) @id
    username String
    email String
    password String
    firstName String?
    lastName String?

    @@map("users")
}